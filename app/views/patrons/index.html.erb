<script>
var commonMaxLen=60;


var clientCountry='United States';
var clientState='NY';
function confirmDeletion(dataName){
	return confirm("Are you sure you want to delete this "+dataName+"? This change cannot be undone.");
}

function changeStateInput(containerObj) {
	var rowIndex=0;
	var tbodies=$('tbody',containerObj.parentNode);
	for(var i=0;i<tbodies.length;i++){
		if(containerObj==tbodies[i]){
			rowIndex=i;
			break;
		}
	}
	
	theCountry = $('#selectCountry',containerObj)[0].value;
	theCountry = theCountry.replace(" ", "_");

	prevEl = $('#'+previousActiveStatesElementIds[rowIndex],containerObj)[0];

	prevEl.style.display='none';
	prevEl.disabled = true;

	states = $("#states_" + theCountry,containerObj)[0];

	if(states == null) {
		states = $('#selectIntl',containerObj)[0];
		previousActiveStatesElementIds[rowIndex] = "selectIntl";
	} else
		previousActiveStatesElementIds[rowIndex] = "states_" + theCountry;

	states.style.display='inline';
	states.disabled = false;

}

function changeUsageType(ctl){
	if(ctl.checked){
		$('input[name=usageType]',ctl.parentNode.parentNode.parentNode).val(ctl.value);
	}
}

function showTableHead(table){
	var tHead=$('thead',table)[0];
	if(tHead!=null){
		if($('tbody',table).length==0){
			tHead.style.display='none';
		}else{
			tHead.style.display='';
		}
	}
}

function addAddress(valueMap){
	var addressTable=$id('addressTable');

	var tbodyCount=$('tbody',addressTable).length;
	var maxCount=5;
	if(tbodyCount>=maxCount){
		alert("Consumer can only have "+maxCount+" addresses.");
		return;
	}

	var tbody=document.createElement("tbody");
	addressTable.appendChild(tbody);
	var tempHTML=$id('addressTemplate').innerHTML;

	copyTbody($id('addressTemplate'),tbody);

	var rowNum=$('tbody',addressTable).length-1;

	if(BrowserDetect.browser=='Explorer'){
		$('td[myType=usageType_display]',tbody).each(function(){
			var name="usageType_display_"+rowNum;
		
			var oldInput=$('input',this)[0];
			var value=oldInput.value;

			var input=null;
		
			//have to recreate this element and set name here, otherwise all radios will be considered as one group(bug of IE).
			input=document.createElement("<input type='radio' name='"+name+"'  class='checkRadio' value='"+value+"'>");

			input.onclick=function(){changeUsageType(this)}; 

			this.innerHTML="";

			this.appendChild(input);
			
		});
	}else{
		$('input[name=usageType_display]',tbody).each(function(){
			this.name=this.name+"_"+rowNum;
		});
	}

	previousActiveStatesElementIds[rowNum]='selectIntl';

	if(valueMap==null){
		valueMap=new Object();
	}

	$('input[name=addressId]',tbody).val(valueMap['addressId']);

	var usageTypeDisplays=$('input[type=radio]',tbody);
	setRadioCheckBoxValue(usageTypeDisplays,valueMap['usageType']);
	usageTypeDisplays.each(function(){
		this.onclick();
	});

	$('select[name=type]',tbody).val(valueMap['type']);
	$('input[name=line1]',tbody).val(valueMap['line1']);
	$('input[name=line2]',tbody).val(valueMap['line2']);
	$('input[name=city]',tbody).val(valueMap['city']);
	$('input[name=zipcode]',tbody).val(valueMap['zipcode']);

	var countries=$('select[name=country]',tbody);
	countries.val(valueMap['country']);
	countries.each(function(){
		this.onchange();
	});

	$('input[name=state],select[name=state]',tbody).val(valueMap['state']);

	showTableHead(addressTable);
}

function deleteAddress(tbody){
	if(confirmDeletion("address")){
		var id=$('input[name=addressId]',tbody)[0].value;
		if(id==null || id==""){
			$(tbody).remove();
		}else{
			window.location="operatePatron.html?method=removeAddress&patronId=43614645&edit=ci&deleteId="+id;
		}
		//tbody.parentNode.removeChild(tbody);
		//showTableHead(tbody.parentNode);
	}
}

function setRadioCheckBoxValue(controls,value){
	if(controls!=null){
		for(var i=0;i<controls.length;i++){
			//alert(controls[i]+":"+controls[i].value+":"+value);
			controls[i].checked=controls[i].value==value;
		}
	}
}

function validateAddresses(form){
	var result=true;
	var tbodies=$('#addressTable tbody');

	if(tbodies!=null){
		for(var i=0;i<tbodies.length;i++){
			//IE7 will have a empty tbody if not specified
			if(tbodies[i].childNodes.length>0){
				result=validateAddress(tbodies[i]);
				if(result==false){
					break;
				}
			}
		}
	}
	return result;
}

