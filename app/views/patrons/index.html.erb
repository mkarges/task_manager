	$('input[name=phoneId]',tbody).val(valueMap['phoneId']);
	var phonePrimaryDisplays=$('input[name=phonePrimaryDisplay]',tbody);
	phonePrimaryDisplays.attr({checked : valueMap['phonePrimary']!=null && valueMap['phonePrimary']==valueMap['phoneId'], value : valueMap['phoneId']});
	phonePrimaryDisplays.each(function(){
		this.onclick();
	});

	$('select[name=phoneType]',tbody).val(valueMap['phoneType']);
	$('input[name=phone]',tbody).val(valueMap['phone']);

	showTableHead(tbody.parentNode);
}

