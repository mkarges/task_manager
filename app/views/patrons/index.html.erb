//Global variable
var continueCapacityControl=true;


function hideMessageDiv(messageCtl){
	if(messageCtl!=null){
		messageCtl.innerHTML="";
		messageCtl.style.display="none";
	}
}

/* Global Parameters
 * submitButtonId: can be null.
 * delegateButtonId: can be null.
 * messageDivId: can be null.
 */
function CapacityControlParams(instanceKey, groupKey, submitButtonId, delegateButtonId, messageDivId){
	this.instanceKey=instanceKey;
	this.groupKey=groupKey;
	this.submitButtonId=submitButtonId;
	this.delegateButtonId=delegateButtonId;
	this.messageDivId=messageDivId;

	//global variables, ids for setTimeout()
	this.checkStatusTimeoutId;
	this.updateTimerTimeoutId;
}

function getRemoteCallParams(paramsObj){
	var params;
	if(paramsObj.groupKey==null){
		params=[paramsObj.instanceKey];
	}else{
		params=[paramsObj.instanceKey,paramsObj.groupKey];
	}
	return params;
}

function checkStatusOnCapacityControl(globalParamsName, onFalseFuncStr, debugForHold){
	var paramsObj=eval(globalParamsName);
	var submitButton=$id(paramsObj.submitButtonId);
	if(submitButton!=null){
		submitButton.disabled=true;
		submitButton.className="disabled";
	}
	try{
		var params=getRemoteCallParams(paramsObj);
		ajax("ajaxManager.getCapacityControlInfo",params,
			function (replay){
				if(replay.error){
					alertError("internal server error!  "+replay.message);
					return;
				}else{
					if(debugForHold){
						return;
					}

					var info=replay.result;
					//alert(info);
					var messageCtl=$id(paramsObj.messageDivId);
					var retryInterval=CAPACITY_CONTROL_RETRY_INTERVAL;
					var delegateButton=$id(paramsObj.delegateButtonId);
					if(info.inHouse){
						if(delegateButton!=null){							
							if(paramsObj.submitForm!=null){
//								alert("submitForm:"+paramsObj.submitForm);
								paramsObj.submitForm();
							}else{
								delegateButton.click();
							}
						}
						hideMessageDiv(messageCtl);
					}else{
						var quitCapacityControlFunc="quitCapacityControl(\""+globalParamsName+"\")";
						if(messageCtl!=null){
							var message="";
							if(info.purged){
								message+="<div class='error'>Your request has been purged from the queue due to your browser is not active for too long.</div>";
							}
							message+="Your request has been queued. Please wait.<BR>"
								+" Requests are processed in the order in which they are received.<BR>" 
								+" This page will automatically refresh in \n"
								+" <span id='retryTimeSpan' style='font-weight: bold; font-size: small;'>"+retryInterval+"</span> seconds\n"
								+"<div align='center'><button type='button' class='submit' onclick='"+quitCapacityControlFunc+"'>Cancel</button></div>";
							messageCtl.innerHTML=message;
							messageCtl.style.display="";
							updateTimerPeriodically('retryTimeSpan',retryInterval,globalParamsName);
						}
						window.onunload=function(){
							eval(quitCapacityControlFunc);
						}
						if(onFalseFuncStr!=null){
							eval(onFalseFuncStr);
						}
					}
				}
			}
		);
		
	}catch(e){    
		alertError("found error!  "+e);
	}
}
function updateTicketLastTime(){
	var result=false;
	try{
		var params=[];
		ajax("ajaxManager.updateTicketLastTime",params,
			function (replay){
				if(replay.error){
					alertError("internal server error!  "+replay.message);
					result=false;
				}else{
					result=true;
				}
			}
		);
		
	}catch(e){    
		alertError("found error!  "+e);
	}
	return result;

}

function updateTimerPeriodically(retryTimeSpanId,value,globalParamsName){
	if(!continueCapacityControl) return;
	var paramsObj=eval(globalParamsName);
	$id(retryTimeSpanId).innerHTML=""+value;
	if(value>1){
		var timeoutId=setTimeout("updateTimerPeriodically('"+retryTimeSpanId+"',"+(value-1)+",'"+globalParamsName+"')",1000);
		paramsObj.updateTimerTimeoutId=timeoutId;
	}
}

function checkStatusOnCapacityControlPeriodically(globalParamsName, restTryTimes){
	if(!continueCapacityControl) return;
	if(restTryTimes>0){
		var onFalseFuncStr="var timeoutId=setTimeout(\"checkStatusOnCapacityControlPeriodically('"+globalParamsName+"',"+(restTryTimes-1)+")\","+CAPACITY_CONTROL_RETRY_INTERVAL+"*1000);"
			+"var paramsObj=eval('"+globalParamsName+"');"
			+"paramsObj.checkStatusTimeoutId=timeoutId;";
		checkStatusOnCapacityControl(globalParamsName, onFalseFuncStr);
		if(restTryTimes % CAPACITY_CONTROL_UPDATE_TICKET_TRY_TIMES ==0){
			updateTicketLastTime();
		}
	}
}

function performCapacityCheck(globalParamsName){
	if(!isCookieEnabled()){
		alertIfCookieDiabled();
		return false;
	}
	continueCapacityControl=true;
	checkStatusOnCapacityControlPeriodically(globalParamsName,CAPACITY_CONTROL_UPDATE_MAX_TRY_TIMES);
}

function quitCapacityControl(globalParamsName){
	var paramsObj=eval(globalParamsName);

	continueCapacityControl=false;

	var timeoutIds=[paramsObj.checkStatusTimeoutId,paramsObj.updateTimerTimeoutId];
	//alert(timeoutIds);
	for(var i=0;i<timeoutIds.length;i++){
		var timeoutId=timeoutIds[i];
		if(timeoutId!=null){
			window.clearTimeout(timeoutId);
		}
	}

	hideMessageDiv($id(paramsObj.messageDivId));
	
	var submitButton=$id(paramsObj.submitButtonId);
	if(submitButton!=null){
		submitButton.disabled=false;
		submitButton.className="submit";
	}
	var result=false;
	try{
		var params=getRemoteCallParams(paramsObj);
		ajax("ajaxManager.removeFromCapacityControl",params,
			function (replay){
				if(replay.error){
					alertError("internal server error!  "+replay.message);
					result=false;
				}else{
					result=true;
				}
				//alert("quitCapacityControl:"+result);
			}
		);
		
	}catch(e){    
		alertError("found error!  "+e);
	}
	return result;
}

function isCookieEnabled(){
	var result=true;
	if(navigator!=null){ 
		result=navigator.cookieEnabled;
	}
	return result;
}
function alertIfCookieDiabled(){
	if(!isCookieEnabled()){
		alert("This site requires cookies. Please adjust your browser settings to enable cookies. Then, refresh this page to continue.");
	}
}
